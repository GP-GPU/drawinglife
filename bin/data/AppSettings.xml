<?xml version="1.0" encoding="UTF-8"?>
<data>
	<database>test.sqlite</database>
	<person>
		<name>Dan</name>
		<name>Sophia</name>
	</person>
</data>
<dbquery>
	<!-- There are four query types: -->
	<!-- 1 = day range -->
	<!-- 2 = month range -->
	<!-- 3 = year range -->
	<!-- 4 = city -->
	<type>3</type>
	<time>
		<yearstart>2008</yearstart>
		<yearend>2010</yearend>
	</time>
	<city>Berlin</city>
</dbquery>
<settings>
	<log level="0" />
	<loadgpsonstart>1</loadgpsonstart>
	<debugmode>0</debugmode>
	<drawspeed>2</drawspeed>
	<framerate>30</framerate>
	<boundingbox>
		<size>10000.0</size>	
		<padding>1000.0</padding>
  	</boundingbox>
</settings>
<ui>
	<fonts>
		<font name="mono.ttf" size="50" />
		<font name="mono.ttf" size="24" />
		<font name="mono.ttf" size="16" />
		<!-- <font name="Standard0753.ttf" size="8" /> -->
		<font name="consola.ttf" size="11" />
	</fonts>
	<alpha>
		<tracks>64</tracks>
		<dots>200</dots>
		<legend>255</legend>
	</alpha>
	<color>
		<foreground r="255" g="255" b="255" />
