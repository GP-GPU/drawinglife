<?xml version="1.0" encoding="UTF-8"?>
<data>
	<database>test.sqlite</database>
	<person>
		<name r="0" g="255" b="0" >Dan</name>
		<!-- <name r="255" g="0" b="0" >Sophia</name> -->
	</person>
</data>
<dbquery>
	<!-- There are four query types: -->
	<!-- 1 = day range -->
	<!-- 2 = month range -->
	<!-- 3 = year range -->
	<!-- 4 = city -->
	<type>4</type>
	<time>
		<yearstart>2007</yearstart>
		<yearend>2009</yearend>
	</time>
	<city>Berlin</city>
</dbquery>
<settings>
	<interactivemode>
		<enabled>1</enabled>
		<traced>1</traced>
	</interactivemode>
	<log level="0" />
	<loadgpsonstart>1</loadgpsonstart>
	<debugmode>0</debugmode>
	<printvalues>1</printvalues>
	<fullscreen>0</fullscreen>
	<hidecursor>1</hidecursor>
	<drawspeed>2</drawspeed>
	<framerate>30</framerate>
	<!-- This sets the number of points to draw.-->
	<!-- To draw all points set 0 (very cpu intensive) -->
	<walklength>0</walklength>
	<boundingbox>
		<enabled>0</enabled>
		<size>20000.0</size>	
		<padding>2000.0</padding>
  	</boundingbox>
	<meridian>
		<auto>1</auto>
		<!-- value in degrees -->
		<lon0>0.0</lon0>
	</meridian>
</settings>
<ui>
	<fonts>
		<font name="mono.ttf" size="50" />
		<font name="mono.ttf" size="24" />
		<font name="mono.ttf" size="16" />
		<!-- <font name="Standard0753.ttf" size="8" /> -->
		<font name="consola.ttf" size="11" />
	</fonts>
	<alpha>
		<tracks>64</tracks>
		<dots>200</dots>
		<legend>255</legend>
	</alpha>
	<color>
		<foreground r="255" g="255" b="255" />
		<background r="0" g="0" b="0" />
		<viewbox r="30" g="30" b="30" />
		<interactiveseg r="255" g="255" b="0" />
	</color>
	<imageascurrent>0</imageascurrent>
	<images>
		<image></image>
		<image></image>
	</images>
	<dotsize>5</dotsize>
</ui>
